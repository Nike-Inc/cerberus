<FindBugsFilter>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="CRLF_INJECTION_LOGS "/>
    </Match>
    <Match>
        <Class name="~.*"/>
        <Bug pattern="SE_NO_SERIALVERSIONID "/>
    </Match>
    <Match>
        <Class name="com.nike.cerberus.api.GatewaySslSocketFactory"/>
        <Bug pattern="RANGE_ARRAY_INDEX "/>
    </Match>
    <Match>
        <Class name="com.nike.cerberus.api.CerberusCompositeApiActions"/>
        <Bug pattern="RV_RETURN_VALUE_IGNORED_NO_SIDE_EFFECT "/>
    </Match>



</FindBugsFilter>
