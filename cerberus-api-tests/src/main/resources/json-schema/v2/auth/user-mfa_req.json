{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "status": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "user_id": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "state_token": {
          "type": "string"
        },
        "devices": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": {
                "type": "string"
              },
              "name": {
                "type": "string"
              }
            },
            "required": [
              "id",
              "name"
            ]
          }
        },
        "client_token": {
          "type": "null"
        }
      },
      "required": [
        "user_id",
        "username",
        "state_token",
        "devices",
        "client_token"
      ]
    }
  },
  "required": [
    "status",
    "data"
  ]
}